#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/var/www/html/s15/node_modules/.pnpm/@biomejs+biome@1.9.2/node_modules/@biomejs/biome/bin/node_modules:/var/www/html/s15/node_modules/.pnpm/@biomejs+biome@1.9.2/node_modules/@biomejs/biome/node_modules:/var/www/html/s15/node_modules/.pnpm/@biomejs+biome@1.9.2/node_modules/@biomejs/node_modules:/var/www/html/s15/node_modules/.pnpm/@biomejs+biome@1.9.2/node_modules:/var/www/html/s15/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/var/www/html/s15/node_modules/.pnpm/@biomejs+biome@1.9.2/node_modules/@biomejs/biome/bin/node_modules:/var/www/html/s15/node_modules/.pnpm/@biomejs+biome@1.9.2/node_modules/@biomejs/biome/node_modules:/var/www/html/s15/node_modules/.pnpm/@biomejs+biome@1.9.2/node_modules/@biomejs/node_modules:/var/www/html/s15/node_modules/.pnpm/@biomejs+biome@1.9.2/node_modules:/var/www/html/s15/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/biome" "$@"
else
  exec node  "$basedir/../../bin/biome" "$@"
fi
